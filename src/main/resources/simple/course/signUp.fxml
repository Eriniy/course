<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="simple.course.SignUpController">
   <children>
      <TextField fx:id="tfLogin" layoutX="50.0" layoutY="126.0" onAction="#handleButtonAction" prefHeight="35.0" prefWidth="299.0" promptText="Логин">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <TextField fx:id="tfPass" layoutX="50.0" layoutY="180.0" onAction="#handleButtonAction" prefHeight="35.0" prefWidth="299.0" promptText="Пароль">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <TextField fx:id="tfName" layoutX="50.0" layoutY="70.0" onAction="#handleButtonAction" prefHeight="35.0" prefWidth="299.0" promptText="Имя">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <Button fx:id="signUpButton" layoutX="105.0" layoutY="269.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="52.0" prefWidth="190.0" text="Зарегистрировать">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="returnButton" layoutX="65.0" layoutY="340.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="39.0" prefWidth="269.0" text="Вернуться на главную">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
